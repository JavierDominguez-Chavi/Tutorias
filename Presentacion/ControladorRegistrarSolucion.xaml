<Page x:Class="Presentacion.ControladorRegistrarSolucion"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Presentacion"
        mc:Ignorable="d"
        Title="ControladorRegistrarSolucion" Height="450" Width="800">
    <Grid>
        <Button x:Name="btnRegistrarSolucion" Content="Registrar solución" HorizontalAlignment="Left" Margin="595,381,0,0" VerticalAlignment="Top" Click="btnRegistrarSolucion_Click"/>
        <TextBox x:Name="tbSolucion" HorizontalAlignment="Left" Margin="76,268,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="628" Height="100" Visibility="Hidden"/>
        <Button x:Name="btnAceptar" Content="Aceptar" HorizontalAlignment="Left" Margin="519,373,0,0" VerticalAlignment="Top" Height="28" Width="67" Visibility="Hidden" Click="btnAceptar_Click"/>
        <Button x:Name="Button_Cancelar" Content="Cancelar" HorizontalAlignment="Left" Margin="619,372,0,0" VerticalAlignment="Top" Height="28" Width="68" Visibility="Hidden" Click="Button_Cancelar_Click"/>
        <TextBlock
                Name="TextBloc_Error"
                Text=""
                Foreground="Red"
                TextWrapping="Wrap"
                Margin="248,28,245,376"/>
    </Grid>
</Page>
